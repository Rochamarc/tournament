CREATE TABLE IF NOT EXISTS retired ( id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, name TEXT NOT NULL, nationality TEXT NOT NULL, age INTEGER NOT NULL, overall INTEGER NOT NULL, last_club TEXT, position VARCHAR(30) NOT NULL, matches_played INTEGER, goals INTEGER, assists INTEGER, points REAL, avg REAL, last_market_value INTEGER,  last_salary REAL,  height REAL,  weight REAL,  foot VARCHAR(10) ) 