CREATE TABLE IF NOT EXISTS players ( id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, name TEXT NOT NULL, nationality TEXT NOT NULL, age INTEGER NOT NULL, overall INTEGER NOT NULL, current_club TEXT, position VARCHAR(30) NOT NULL, matches_played INTEGER, goals INTEGER, assists INTEGER, points REAL, avg REAL, market_value INTEGER,  salary REAL,  height REAL,  weight REAL,  foot VARCHAR(10), save_file VARCHAR(30), retirement TEXT);