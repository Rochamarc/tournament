CREATE TABLE retired ( id INT NOT NULL PRIMARY KEY AUTO_INCREMENT, name VARCHAR(100) NOT NULL, nationality VARCHAR(100) NOT NULL, age INT(2) NOT NULL, overall INT(2) NOT NULL, last_club VARCHAR(100), position VARCHAR(2) NOT NULL, matches_played INT(4), goals INT(4), assists INT(4), points FLOAT(3,2), avg FLOAT(3,2), last_market_value INT(10),  last_salary FLOAT(10,2),  height FLOAT(2,2),  weight FLOAT(2,2),  foot VARCHAR(5), save_file VARCHAR(30));